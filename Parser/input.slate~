\section{third section}

\begin{slide}[supportbin=abc]{Multi Columned Text}
\begin{columns}
\begin{column}[align=t]{0.5%}
\begin{block}
\title{Block 1}
Hi, I am normal text
\\Text can be resized

\end{block}
\end{column}
\begin{column}[align=t]{0.5%}
\begin{itemize}
	\item<1-> enum1
	\item<2-> enum2
	\item
		\begin{itemize}
			\item<3-> enum21
			\item<4-> enum22
				\begin{enumerate}
					\item<5-> enum221
					\item<6-> enum222
				\end{enumerate}
      \item<7-> enum23
      \item<8-> enum24
				\begin{itemize}
					\item<9-> enum241
					\item<10-> enum242
				\end{itemize}
      \item<11-> enum25
      
		\end{itemize}
\end{itemize}


\end{column}
\end{columns}
\end{slide}

\begin{slide}{Multi Columned Text}
\begin{columns}
\begin{column}[align=t]{0.5%}
\begin{block}<1->
\title{Block 1}
Hi, I visible from 1
\end{block}
\begin{block}<3-7|covered>
\title{Block 2}
Hi, I visible from 3 to 7 else covered
\end{block}
\begin{block}<3-7>
\title{Block 3}
Hi, I visible from 3 to 7 only
\end{block}
\begin{block}<2-8>
\title{Block 4}
Hi, I visible from 2 to 8
\end{block}
\begin{block}<-3>
\title{Block 5}
Hi, I visible upto 3
\end{block}
\end{column}
\begin{column}[align=t]{0.5%}
\begin{itemize}
	\item<1-> enum1
	\item<2-> enum2
	\item
		\begin{itemize}
			\item<3-> enum21
			\item<4-> enum22
				\begin{enumerate}
					\item<5-> enum221
					\item<6-> enum222
				\end{enumerate}
      \item<7-> enum23
      \item<8-> enum24
				\begin{itemize}
					\item<9-> enum241
					\item<10-> enum242
				\end{itemize}
      \item<11-> enum25
      
		\end{itemize}
\end{itemize}


\end{column}
\end{columns}
